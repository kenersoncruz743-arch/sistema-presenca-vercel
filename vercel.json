{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/menu",
      "dest": "/public/menu.html"
    },
    {
      "src": "/painel",
      "dest": "/public/painel.html"
    },
    {
      "src": "/coletores",
      "dest": "/public/coletores.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
